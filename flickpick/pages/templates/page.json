"page": {{ page.name }},


	"widgets": {
	{% for widget in page.widgets %}

		"widget": {{ widget.name }},
		"limit": {{ widget.movieswidget.limit }},
		"display_type": {{ widget.movieswidget.display_type }},
		{% for source in widget.movieswidget.sources %}
			"source": {{ source }},
		{% endfor %}

	{% endfor %}
}